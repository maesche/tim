<configuration>
	<system>
		<db-user>tim</db-user>
		<db-pwd>tim</db-pwd>
		<db-url>jdbc:mysql://localhost:3306/tim?autoReconnect=true</db-url>
		<db-driver>com.mysql.jdbc.Driver</db-driver>
		<default-language>fr</default-language>
		<date-format>yyyy-MM-dd</date-format>
