<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<system>
		<db-user>tim</db-user>
		<db-pwd>tim</db-pwd>
		<db-url>jdbc:mysql://localhost:3306/tim?autoReconnect=true</db-url>
		<db-driver>com.mysql.jdbc.Driver</db-driver>
		<default-lang>fr</default-lang>
		<date-format>yyyy-MM-dd</date-format>
		<time-format>HH:mm</time-format>
	</system>
	<calendar>
		<day-start>9</day-start>
		<day-end>19</day-end>
		<day-interval>15</day-interval>
	</calendar>
</configuration>

